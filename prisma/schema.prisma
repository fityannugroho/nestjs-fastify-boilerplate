// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql" // Change to your preferred database provider
  url      = env("DATABASE_URL")
}

// Example model, remove or modify it as needed
model User {
  id    Int     @id @default(autoincrement())
  email String  @unique
  name  String?
}
